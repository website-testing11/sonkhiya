                navigator.userAgent.match(/BlackBerry/i) ||
                navigator.userAgent.match(/Windows Phone/i)
            ) {
                $(".slick-slider-testimonial-Salepage").slick({
                slidesToShow: 1,
                infinite: true,
                slidesToScroll: 1,
                autoplay: true,
